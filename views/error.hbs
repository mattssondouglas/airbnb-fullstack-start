  <body>
    <nav class="topBanner">
      <div class="container my-2">
        <div class="row">
          <div class="col">
            <img src="./images/logo-airbnb.png" alt="Airbnb" />
          </div>
          <div class="col">
            <div class="row text-end">
              <div class="col text-end">
                <button type="button" class="btn btn-light">
                  <img
                    src="https://randomuser.me/api/portraits/men/11.jpg"
                    class="rounded-circle avatarimg"
                    alt="avatar login"
                  />
                  <span>Douglas</span>
                </button>
                <button type="button" class="btn btn-light">
                  Log out
                </button>
              </div>
            </div>
          </div>
        </div>
      </div>
    </nav>
    <div class="container mt-4">
      <div class="card bg-danger" style="--bs-bg-opacity: .2;">
        <div class="card-body">
          <span>{{message}}</span>
        </div>
      </div>
      <div class="error-text mt-4">
        <p>
					{{error.stack}}
        </p>
      </div>
    </div>
  </body>
